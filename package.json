{
  "name": "youtube-jukebox",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "node ./server/devServer",
    "start": "cross-env NODE_ENV=production node ./server/prodServer",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf build",
    "build": "npm run lint && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --color -p --progress",
    "precommit": "lint-staged",
    "lint": "standard"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "expect",
      "it",
      "test",
      "describe"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --semi false --single-quote",
      "standard --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "redbox-react": "^1.4.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "firebase": "^4.1.3",
    "google-auth-library": "^0.10.0",
    "googleapis": "^19.0.0",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "3.0.2",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "readline": "^1.3.0",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0"
  }
}
